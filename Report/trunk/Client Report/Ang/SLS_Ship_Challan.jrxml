	<queryString>
		<![CDATA[select

shipmnt_id,
  so_id,
  itm_desc,
  ship_itm_id,
  uom,
  PICK_QTY,
  itm_qty,
  ship_qty,
  BAL_QTY,
  ITM_AMT_BS,
  base_price,
  ITM_AMT_SP,
  pick_id,
  ORG_DESC,
  ADDS,
  org_tin_no,
  cust_name,
  cust_id,
  cust_tin_no,
  cust_addrs,
  consignee_name,
  consignee_addrs,
  transporter_name,
  TRANSPORTER_Id,
  TRANS_MODE,
  tmode,
  TRANS_VH_NO,
  TRANS_GR_NO,
  TRANS_CONTNR_NO,
  REMARKS,
  EXT_DOC_NO,
  --
  so_doc_id,
  SCHM_ID,
  USR_NAME,
  SR_NO,
  SHIPMNT_QTY,
  REG_NO,
  CIN_NO,
  MR_NO,
  PO_NO,
  eo_per_nm,
  eo_phone,
  ship_adds,
  ho_org_id,
  ext_doc_dt,
  doc_dt,assigned_to,emp_location,emp_phone1
--  scheme
  --  b.sno,
  --  b.schm_itm_id,
  --  b.sitm_desc,
  --  b.CLD_ID,
  --  b.ORG_ID,
  --  b.doc_id,
  --  b.schm_uom,
  --  b.schm_price,
  --  b.so_id,
  --  b.schm_val,
  --  b.free_qty,
  --  b.HO_ORG_ID,
  --  b.SLOC_ID,b.so_doc_id,
  ,
(select sum(nvl(itm_amt_sp,0)) from sls$shipmnt a,sls$shipmnt$itm b
   WHERE a.CLD_ID               = b.CLD_ID
  AND a.SLOC_ID                = b.SLOC_ID
  AND a.ORG_ID                 = b.ORG_ID
  AND a.HO_ORG_ID              = b.HO_ORG_ID
  AND a.DOC_ID                 = b.DOC_ID
  and a.pick_id		= b.pick_id
  and a.DOC_DT                >= NVL(TO_DATE($P{FromDate}, 'yyyy-MM-dd'), a.DOC_DT)
  AND a.DOC_DT                <= NVL(TO_DATE($P{ToDate}, 'yyyy-MM-dd'), a.DOC_DT)
  AND b.ITM_ID                 = NVL($P{ItmId}, b.ITM_ID)
  AND a.DOC_ID                 = NVL($P{DocId}, a.DOC_ID)
  AND NVL(a.SHIPMNT_STAT, 217) = NVL($P{Status}, NVL(a.SHIPMNT_STAT, 217))
  AND a.CLD_ID                 = NVL($P{CldId}, a.CLD_ID)
  AND a.SLOC_ID                = NVL($P{SlocId}, a.SLOC_ID)
  AND a.ORG_ID                 = NVL($P{OrgId}, a.ORG_ID)
  AND a.HO_ORG_ID              = NVL($P{HoOrgId}, a.HO_ORG_ID)
  AND a.CURR_ID_SP             = NVL($P{CurrId}, a.CURR_ID_SP)
  AND a.EO_ID                  = NVL($P{EoId}, a.EO_ID))tot_amt,engineer
from
(
SELECT shipmnt_id,
  so_id,
  itm_desc,
  ship_itm_id,
  uom,
  PICK_QTY,
  itm_qty,
  ship_qty,
  BAL_QTY,
  ITM_AMT_BS,
  base_price,
  ITM_AMT_SP,
  pick_id,
  ORG_DESC,
  ADDS,
  org_tin_no,
  cust_name,
  cust_id,
  cust_tin_no,
  cust_addrs,
  consignee_name,
  consignee_addrs,

  TRANSPORTER_Id,

  tmode,
  TRANS_VH_NO,

  TRANS_CONTNR_NO,
  REMARKS,
  EXT_DOC_NO,
  --
  so_doc_id,
  SCHM_ID,
  USR_NAME,
  SR_NO,
  SHIPMNT_QTY,
  REG_NO,
  CIN_NO,
  MR_NO,
  PO_NO,
  eo_per_nm,
  eo_phone,
  ship_adds,
  ho_org_id,
  ext_doc_dt,
  doc_dt
--  scheme
  --  b.sno,
  --  b.schm_itm_id,
  --  b.sitm_desc,
  --  b.CLD_ID,
  --  b.ORG_ID,
  --  b.doc_id,
  --  b.schm_uom,
  --  b.schm_price,
  --  b.so_id,
  --  b.schm_val,
  --  b.free_qty,
  --  b.HO_ORG_ID,
  --  b.SLOC_ID,b.so_doc_id
,assigned_to,emp_location,emp_phone1,Transport_Name_Flx Transporter_Name
,Transport_Mode_Flx TRANS_MODE,Transport_Gr_No_Flx TRANS_GR_NO,
engineer
FROM
  (SELECT DISTINCT SUBSTR(a.SHIPMNT_ID, 3) shipmnt_id,
    SUBSTR(c.SO_ID, 3) so_id,
--    a.DOC_DT,
    g.ITM_DESC,
    b.ITM_ID ship_itm_id,
    (SELECT l.UOM_DESC
    FROM app.app$uom$conv$std l
    WHERE l.CLD_ID = b.CLD_ID
    AND l.SLOC_ID  = b.SLOC_ID
    AND l.UOM_ID   = b.ITM_UOM
    ) uom,
    c.DOC_ID so_doc_id,
    a.org_id,
    a.sloc_id,
    a.cld_id,
    b.PICK_QTY,
    b.ITM_QTY_BS itm_qty,
    b.ITM_QTY_BS ship_qty,
    b.BAL_QTY,
    b.ITM_AMT_BS,
    ROUND(NVL(b.ITM_AMT_SP, 0) / NVL(b.ITM_QTY_BS, 0), 2) base_price,
    b.ITM_AMT_SP,
    SUBSTR(h.PICK_ID, 3) pick_id,
    d.ORG_DESC,
    d.ADDS,
    d.TIN_NO org_tin_no,
    e.EO_NM cust_name,
    e.EO_ID cust_id,
    (SELECT app.app$eo$prf.TIN_NO
    FROM app.app$eo$prf
    WHERE app.app$eo$prf.EO_CLD_ID  = e.EO_CLD_ID
    AND app.app$eo$prf.EO_SLOC_ID   = e.EO_SLOC_ID
    AND app.app$eo$prf.EO_HO_ORG_ID = e.EO_HO_ORG_ID
    AND app.app$eo$prf.EO_ID        = e.EO_ID
    ) cust_tin_no,
    (SELECT k.ADDS
    FROM app.app$eo$adds l,
      app.app$adds$vw k
    WHERE l.CLD_ID  = k.CLD_ID
    AND l.SLOC_ID   = k.SLOC_ID
    AND l.ADDS_ID   = k.ADDS_ID
    AND l.BILL_ADDS_DFLT = 'Y'
    AND L.BILL_ADDS = 'Y'
    AND l.CLD_ID    = e.EO_CLD_ID
    AND l.SLOC_ID   = e.EO_SLOC_ID
    AND l.HO_ORG_ID = e.EO_HO_ORG_ID
    AND l.EO_ID     = e.EO_ID
    AND ROWNUM = 1
    ) cust_addrs,
    (SELECT DISTINCT m.EO_NM
    FROM app.app$eo m
    WHERE k.CLD_ID  = m.EO_CLD_ID
    AND k.SLOC_ID   = m.EO_SLOC_ID
    AND k.ORG_ID    = m.EO_ORG_ID
    AND k.HO_ORG_ID = m.EO_HO_ORG_ID
    AND k.EO_ID     = m.EO_ID
    AND m.EO_TYPE   = 68
    ) consignee_name,
    (SELECT DISTINCT y.ADDS
    FROM app.app$eo$adds x,
      app.app$adds$vw y,
      app.app$eo z
    WHERE x.CLD_ID       = y.CLD_ID
    AND x.SLOC_ID        = y.SLOC_ID
    AND x.ADDS_ID        = y.ADDS_ID
    AND x.CLD_ID         = z.EO_CLD_ID
    AND x.SLOC_ID        = z.EO_SLOC_ID
    AND x.HO_ORG_ID      = z.EO_HO_ORG_ID
    AND x.EO_ID          = z.EO_ID
    AND x.CLD_ID         = z.EO_CLD_ID
    AND x.SLOC_ID        = z.EO_SLOC_ID
    AND x.ORG_ID         = z.EO_ORG_ID
    AND x.HO_ORG_ID      = z.EO_HO_ORG_ID
    AND x.EO_ID          = k.EO_ID
    AND z.EO_TYPE        = 68
    AND x.SHIP_ADDS_DFLT = 'Y'
    ) consignee_addrs,
    (SELECT p.EO_NM
    FROM app.app$eo p
    WHERE p.EO_CLD_ID  = a.CLD_ID
    AND p.EO_SLOC_ID   = a.SLOC_ID
    AND p.EO_HO_ORG_ID = a.HO_ORG_ID
    AND p.EO_ID        = a.TRANSPORTER_ID
    ) transporter_name,
    a.TRANSPORTER_ID,
    a.TRANS_MODE,
    (SELECT v.ATT_NM
    FROM app.app$ds$att v
    WHERE v.ATT_TYPE_ID = 38
    AND v.ATT_ID        = a.TRANS_MODE
    ) tmode,
    a.TRANS_VH_NO,
    a.TRANS_GR_NO,
    a.TRANS_CONTNR_NO,
    a.REMARKS,
    c.EXT_DOC_NO,
    s.SCHM_ID,
    su.USR_NAME,
    ss.SR_NO,
    ss.SHIPMNT_QTY,
    d.REG_NO,
    d.CIN_NO,
    SUBSTR(c.ext_doc_no,0,INSTR(c.ext_doc_no,' ')) MR_NO,
    SUBSTR(c.ext_doc_no,instr(c.ext_doc_no,' ')+1) PO_NO,
    ep.eo_per_nm,
    ep.eo_phone,
    (SELECT app.fn_get_adds_1 ($P{SlocId}, $P{CldId}, a.ship_adds, 'M' )
    FROM DUAL
    ) ship_adds,
    a.ho_org_id,to_char(a.doc_dt,'dd-MM-yyyy')doc_dt,to_char(c.ext_doc_dt,'dd-MM-yyyy')ext_doc_dt,
l.eo_nm assigned_to,l.emp_location,l.emp_phone1,
(select field_val from sls$doc$conf$flx
where field_id = (select flx.fld_id from  app.app$flx$fld flx
                  where UPPER(FLX.FLD_NM) like 'TRANSPORTER NAME%')
AND DOC_ID_src = $P{DocId}
and rownum=1)Transport_Name_Flx,
(select field_val from sls$doc$conf$flx
where field_id = (select flx.fld_id from  app.app$flx$fld flx
                  where FLX.FLD_NM like 'Transporter Mode%')
AND DOC_ID_src = $P{DocId}
and rownum=1)Transport_Mode_Flx,
(select FIELD_VAL from SLS$DOC$CONF$FLX
where field_id = (select flx.fld_id from  app.app$flx$fld flx
                  where FLX.FLD_NM like 'Transporter GR No.%')
AND DOC_ID_src = $P{DocId}
and rownum=1)Transport_Gr_No_Flx,
(select EMP_NM from HCM.ORG$HCM$EMP$PRF PRF
 where EMP_CODE = a.EMP_EO_ID
 and CLD_ID = a.CLD_ID
 AND SLOC_ID = A.SLOC_ID
 and a.ORG_ID = a.ORG_ID
 AND A.HO_ORG_ID = A.HO_ORG_ID
AND ROWNUM = 1)ENGINEER
  FROM sls$shipmnt a,
    sls$shipmnt$itm b,
    sls$so c,
    (SELECT a.ORG_ID,
      b.CIN_NO,
      a.ORG_DESC,
      b.TIN_NO,
      c.ADDS,
      b.REG_NO
    FROM app.org a,
      app.org$oth$dtl b,
      app.app$adds$vw c
    WHERE a.ORG_ID    = b.ORG_ID(+)
    AND a.ORG_CLD_ID  = b.ORG_CLD_ID(+)
    AND b.ORG_ADD_ID1 = c.ADDS_ID(+)
    AND b.ORG_CLD_ID  = c.CLD_ID(+)
    ) D,
    app.app$eo e,
    app.app$eo$pers$comm ep,
    app.app$itm$org f,
    app.app$itm$prf g,
    sls.sls$pick h,
    sls.sls$so$itm i,
    sls.sls$so$itm$schm s,
    sls.sls$intm$slip$itm j,
    sls.sls$intm$slip k,
    app.app$sec$usr su,
    sls.sls$shipmnt$itm$sr ss,

(
    SELECT eo_cld_id,
      x.eo_sloc_id,
      x.eo_ho_org_id,
      x.eo_id,
      x.eo_nm,
      (select param_desc from hcm.hcm$setup
      where param_id = e.emp_loc_id
      and cld_id = e.cld_id
      and sloc_id = e.sloc_id
      and org_id = e.org_id
      and ho_org_id = e.ho_org_id)emp_location,e.emp_phone1
    FROM app.app$eo x,hcm.org$hcm$emp$prf e
    WHERE
    x.eo_cld_id = e.cld_id
    and x.eo_sloc_id = e.sloc_id
    and x.eo_org_id = e.org_id
    AND X.EO_ID = E.EMP_CODE
    and x.eo_cld_id = $P{CldId}
    AND x.eo_sloc_id  = $P{SlocId}
    AND x.eo_type     = '80'
    ) l
  WHERE a.CLD_ID               = b.CLD_ID
  AND a.SLOC_ID                = b.SLOC_ID
  AND a.ORG_ID                 = b.ORG_ID
  AND a.HO_ORG_ID              = b.HO_ORG_ID
  AND a.DOC_ID                 = b.DOC_ID
  AND b.CLD_ID                 = c.CLD_ID
  AND b.SLOC_ID                = c.SLOC_ID
  AND b.ORG_ID                 = c.ORG_ID
  AND b.HO_ORG_ID              = c.HO_ORG_ID
  AND b.SO_ID                  = c.DOC_ID
  AND c.ORG_ID                 = d.ORG_ID
  AND a.CLD_ID                 = e.EO_CLD_ID
  AND a.SLOC_ID                = e.EO_SLOC_ID
  AND a.HO_ORG_ID              = e.EO_HO_ORG_ID
  AND a.EO_ID                  = e.EO_ID
  AND b.CLD_ID                 = f.CLD_ID
  AND b.SLOC_ID                = f.SLOC_ID
  AND b.ORG_ID                 = f.ORG_ID
  AND b.HO_ORG_ID              = f.HO_ORG_ID
  AND b.ITM_ID                 = f.ITM_ID
  AND f.ITM_ID                 = g.ITM_ID
  AND b.CLD_ID                 = h.CLD_ID
  AND b.SLOC_ID                = h.SLOC_ID
  AND b.HO_ORG_ID              = h.HO_ORG_ID
  AND b.ORG_ID                 = h.ORG_ID
  AND b.PICK_ID                = h.DOC_ID
  AND c.CLD_ID                 = i.CLD_ID(+)
  AND c.SLOC_ID                = i.SLOC_ID(+)
  AND c.ORG_ID                 = i.ORG_ID(+)
  AND c.HO_ORG_ID              = i.HO_ORG_ID(+)
  AND c.DOC_ID                 = i.DOC_ID(+)
  AND b.CLD_ID                 = i.CLD_ID(+)
  AND b.SLOC_ID                = i.SLOC_ID(+)
  AND b.ORG_ID                 = i.ORG_ID(+)
  AND b.HO_ORG_ID              = i.HO_ORG_ID(+)
  AND b.ITM_ID                 = i.ITM_ID(+)
  AND b.CLD_ID                 = j.CLD_ID(+)
  AND b.SLOC_ID                = j.SLOC_ID(+)
  AND b.ORG_ID                 = j.ORG_ID(+)
  AND b.HO_ORG_ID              = j.HO_ORG_ID(+)
  AND b.ITM_ID                 = j.ITM_ID(+)
  AND b.DOC_ID                 = j.SHIPMNT_DOC_ID(+)
  AND j.CLD_ID                 = k.CLD_ID(+)
  AND j.SLOC_ID                = k.SLOC_ID(+)
  AND j.ORG_ID                 = k.ORG_ID(+)
  AND j.HO_ORG_ID              = k.HO_ORG_ID(+)
  AND j.DOC_ID                 = k.DOC_ID(+)
  AND i.CLD_ID                 = s.CLD_ID(+)
  AND i.SLOC_ID                = s.SLOC_ID(+)
  AND i.ORG_ID                 = s.ORG_ID(+)
  AND i.HO_ORG_ID              = s.HO_ORG_ID(+)
  AND i.DOC_ID                 = s.DOC_ID(+)
  AND i.ITM_ID                 = s.ITM_ID(+)
  AND a.USR_ID_CREATE          = su.USR_ID
  AND a.SLOC_ID                = su.SLOC_ID
  AND a.CLD_ID                 = ss.CLD_ID(+)
  AND a.SLOC_ID                = ss.SLOC_ID(+)
  AND a.ORG_ID                 = ss.ORG_ID(+)
  AND a.WH_ID                  = ss.WH_ID(+)
  AND a.DOC_ID                 = ss.DOC_ID(+)
  AND a.PICK_ID                = ss.PICK_ID(+)
  AND b.SO_ID                  = ss.SO_ID(+)
  AND b.ITM_ID                 = ss.ITM_ID(+)
  AND e.eo_cld_id              = ep.eo_cld_id(+)
  AND e.eo_sloc_id             = ep.eo_sloc_id(+)
  AND e.eo_ho_org_id           = ep.eo_ho_org_id(+)
  AND e.eo_id                  = ep.eo_id(+)
  AND C.cld_id               = l.eo_cld_id(+)
  AND C.sloc_id              = l.eo_sloc_id(+)
  AND C.ho_org_id            = l.eo_ho_org_id(+)
  AND C.assigned_to          = l.eo_id(+)
  AND a.DOC_DT                >= NVL(TO_DATE($P{FromDate}, 'yyyy-MM-dd'), a.DOC_DT)
  AND a.DOC_DT                <= NVL(TO_DATE($P{ToDate}, 'yyyy-MM-dd'), a.DOC_DT)
  AND b.ITM_ID                 = NVL($P{ItmId}, b.ITM_ID)
  AND a.DOC_ID                 = NVL($P{DocId}, a.DOC_ID)
  AND NVL(a.SHIPMNT_STAT, 217) = NVL($P{Status}, NVL(a.SHIPMNT_STAT, 217))
  AND a.CLD_ID                 = NVL($P{CldId}, a.CLD_ID)
  AND a.SLOC_ID                = NVL($P{SlocId}, a.SLOC_ID)
  AND a.ORG_ID                 = NVL($P{OrgId}, a.ORG_ID)
  AND a.HO_ORG_ID              = NVL($P{HoOrgId}, a.HO_ORG_ID)
  AND g.GRP_ID                 = NVL($P{GrpId}, g.GRP_ID)
  AND a.CURR_ID_SP             = NVL($P{CurrId}, a.CURR_ID_SP)
  AND e.CATG_ID                = NVL($P{EoCatId}, e.CATG_ID)
  AND a.EO_ID                  = NVL($P{EoId}, a.EO_ID)
  AND NVL(c.ASSIGNED_TO, 80)   = NVL($P{AssignTo}, NVL(c.ASSIGNED_TO, 80))
  ORDER BY ITM_DESC,
    SS.SR_NO
  ))]]>
	</queryString>
